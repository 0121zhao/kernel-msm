/ {
	memory {
		#address-cells = <2>;
		#size-cells = <2>;

		ramoops: ramoops@0 {
			linux,reserve-contiguous-region;
			linux,reserve-region;
			reg = <0 0x0e700000 0 0x00200000>;
			label = "ramoops";
		};
	};
};
