/include/ "msm8960ab-motref.dtsi"

/ {
	model = "Motorola MSM8960 Pro Sasquatch";
	compatible = "mmi,msm8960";

	mmi,product = "mmi,msm8960ab-sasquatch";
};

&i2c3 {
	atmxt@4a {
		compatible = "atmel,atmxt-ts";
		reg = <0x4a>;
		interrupt-parent = <&msmgpio>;
		interrupts = <46 0>;
		gpios = <&msmgpio 46 0>, /* IRQ */
		        <&msmgpio 63 0>, /* RESET */
		        <&msmgpio 52 0>; /* ENABLE */

		atmel,atmxt-tdat-filename = "atmxt-r2.tdat";
	};

	sy3400@20 {
		compatible = "synaptics,sy3400";
		reg = <0x20>;
		interrupt-parent = <&msmgpio>;
		interrupts = <46 0>;
		gpios = <&msmgpio 46 0>, /* IRQ */
			<&msmgpio 63 0>; /* RESET */

		always_on_capable = <1>;
		synaptics,sy3400-tdat-filename = "sy3400-r0.tdat";
	};
};
