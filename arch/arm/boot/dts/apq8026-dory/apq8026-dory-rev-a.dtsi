/include/ "apq8026-dory.dtsi"

&soc {
	sound {
		dory,mic-en-gpio = <&msmgpio 60 0x00>;
	};
	i2c@f9924000 {
		lis3dsh_acc@1e {
			lis,negate_x = <0>;
			lis,negate_y = <0>;
			lis,negate_z = <1>;
		};
	};
	i2c@f9927000 {
		synaptics@20 {
			status = "ok";
		};
	};
};
